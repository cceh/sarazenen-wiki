<?xml version="1.0" encoding="UTF-8"?><page>
        <meta>
            <title>Abfassungsregion</title>
            <type>Template</type>
            <id/>
            <categories>
                <category>Template</category>
            </categories>
            <rules>
                <rule/>
            </rules>
        </meta>
        <text>  <noinclude>
                <h2>Tabellarische Übersicht</h2>
                <wikisyntax>{{#ask: [[Abfassungsregion::West- und Mitteleuropa]] 
                [[Kategorie:Werk]]
                |?abgefasst in
                |format=broadtable
                |limit=500
                |offset=0
                |link=all
                |width=auto
                |sort=
                |order=asc
                |headers=show
                |searchlabel=… weitere Ergebnisse
                |class=sortable wikitable smwtable
                }}</wikisyntax>
                <div class="hauptseite-block border" >
                    <h2>Georeferenzierung</h2>
                    <div class="" >
                        {{#compound_query:
                        [[-abgefasst in.Abfassungsregion::West- und Mitteleuropa]]; ?getty_coordinates ; icon=icon-abfassungsort-41.png; limit=9999
                        |format=leaflet
                        |cluster = yes
                        |clustermaxradius=20
                        |width=100%
                        |height=350px
                        |defzoom=3
                        |maxzoom=14
                        |minzoom=3
                        |layers=Esri.WorldShadedRelief
                        |type=terrain
                        }}
                    </div></div>
                <h2>Abfassungszeiten</h2>
                <wikisyntax>{{#ask:
                [[-Has subobject.Abfassungsregion::West- und Mitteleuropa]]
                [[AbfassungszeitraumBeginn::+]]
                |?AbfassungszeitraumBeginn
                |?AbfassungszeitraumEnde
                |timelinestart =AbfassungszeitraumBeginn
                |timelineend=AbfassungszeitraumEnde        
                |timelinesize=500px
                |sort=AbfassungszeitraumBeginn
                |order=descending
                |format=timeline
                |timelinebands= DECADE, CENTURY
                |timelineposition=end
                |limit=9999
                }}</wikisyntax>
            </noinclude> 
                <onlyinclude><wikiysntax>__TOC__</wikiysntax>
